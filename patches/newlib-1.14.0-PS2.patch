+
+   la   $8, _ps2sdk_libc_deinit_weak
+   beqz   $8, 1f
+   nop
+   jalr   $8      # _ps2sdk_libc_deinit()
+   nop
+1:
+
+   j Exit         # Exit(retval) (noreturn)
+   move   $4, $16
+
+   .end   _exit
+
+   .ent   _root
+_root:
+   addiu   $3, $0, 35
+   syscall         # ExitThread() (noreturn)
+   .end   _root
+
+   .bss
+   .align   6
+_args:
+   .space   4+16*4+256   # argc, 16 arguments, 256 bytes payload
